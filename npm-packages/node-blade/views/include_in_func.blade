- locals.parentLocals = true
- for(var i = 1; i <= 3; i++)
	include "include2"
- for(var i = 1; i <= 3; i++)
	include "include2" exposing i
- var include2 = "include2"
- for(var i = 1; i <= 3; i++)
	include include2
- for(var i = 1; i <= 3; i++)
	include include2 exposing i
p The end: #{typeof locals.i} #{typeof i}

p Before test block
block test
	p This will be replaced
p After test block

function replaceIt(includeFile)
	replace block test
		include includeFile

call replaceIt("include2")
